[
    /*
     * Internal functions
     */
    { "key": "cmd+e",           "command": "workbench.action.quickOpenPreviousEditor" },
    { "key": "cmd+g",           "command": "workbench.action.gotoLine" },
    { "key": "cmd+y",           "command": "editor.action.deleteLines",
                                   "when": "editorTextFocus && !editorReadonly" },
    { "key": "ctrl+g",          "command": "editor.action.addSelectionToNextFindMatch",
                                   "when": "editorFocus" },
    { "key": "alt+down",        "command": "editor.action.insertCursorBelow",
                                   "when": "editorTextFocus" },
    /*
     * Plugins
     */
    // Contextual Duplicate
    { "key": "cmd+d",           "command": "lafe.duplicateCode",
                                   "when": "editorTextFocus && !editorReadonly" },
    // Markdown Shortcuts
    { "key": "cmd+k",           "command": "md-shortcut.toggleLink",
                                "when": "editorTextFocus && !editorReadonly"
                            },
                            {
                                "key": "shift+f6",
                                "command": "editor.action.rename",
                                "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
                            },
                            {
                                "key": "f2",
                                "command": "-editor.action.rename",
                                "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
                            }
                        ]
