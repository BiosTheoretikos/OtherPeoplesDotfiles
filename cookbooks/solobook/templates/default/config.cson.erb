"*":
  core:
    allowPendingPaneItems: false
    audioBeep: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      ".keep"
      "._*"
      "*.pyc"
      "Thumbs.db"
    ]
    projectHome: "/home/j/code"
    telemetryConsent: "no"
  editor:
    softWrap: true
    tabLength: 2
    tabType: "soft"
  "exception-reporting":
    userId: "chef-dotfilemanager-needs-to-be-set"
  fonts:
    fontFamily: "Hack"
  "linter-ui-default": {}
  minimap:
    plugins:
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      linter: true
      linterDecorationsZIndex: 0
  welcome:
    showOnStartup: false
